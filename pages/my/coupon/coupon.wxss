.container {
  display: flex;
  flex-direction: column;
  background: #f0f2f5;
}
.unuse-coupon .unuse-item{
  height: 368rpx;
  background: url(http://ovu8osqcv.bkt.clouddn.com/unicorm_bg.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
  margin-top: 10rpx;
}
.unuse-coupon .unuse-item.zaiyi{
  background: url(http://ovu8osqcv.bkt.clouddn.com/zaiyi_bg.png);
  background-repeat: no-repeat;
  background-size: 100% 100%;
}
.unuse-coupon .unuse-item .top-row{
  display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 44rpx 56rpx 0 46rpx;
}
.unuse-coupon .unuse-item .top-row .left-com image{
 width: 106rpx;
 height: 106rpx;
 vertical-align: middle;
}
.unuse-coupon .unuse-item .top-row .left-com text{
  margin-left: 20rpx;
  font-size: 30rpx;
  color: #fff;
}
.unuse-coupon .unuse-item .top-row .right-com .money-num{
  text-align: right;
  font-size: 40rpx;
  color: #fff;
}
.unuse-coupon .unuse-item .top-row .right-com .minus-text{
  margin-top: 10rpx;
  text-align: right;
  font-size: 22rpx;
  color: #fff;
}
.unuse-coupon .unuse-item .bottom-row{
    display: flex;
  flex-direction: row;
  justify-content: space-between;
  padding: 0rpx 56rpx 0 46rpx;
  margin-top: 56rpx;
}
.unuse-coupon .unuse-item .bottom-row .left-com image{
  width: 22rpx;
  height: 22rpx;
  vertical-align: middle;
}
.unuse-coupon .unuse-item .bottom-row .left-com  text{
  margin-left: 10rpx;
  font-size: 22rpx;
  color: #4a4a4a;
  vertical-align: middle;
}
.unuse-coupon .unuse-item .bottom-row .right-com .use-btn{
  margin-top: 15rpx;
  width: 170rpx;
  height: 60rpx;
  line-height: 60rpx;
  background: #b74040;
  font-size: 26rpx;
  color: #fff;
}
.unuse-coupon .unuse-item .bottom-row .right-com .use-btn.purple{
  background: #7a7ad0;
}
.vip-wrapper{
  position: relative;
    margin-top: 10rpx;
  }
  .vip-wrapper button{
 position: absolute;
 right: 60rpx;
 bottom: 70rpx;
 width: 170rpx;
  height: 60rpx;
  line-height: 50rpx;
  border-radius: 0;
  font-size: 22rpx;
  color: #fff;
  }
.vip-wrapper image{
  width: 100%;
  height: 443rpx;
}
